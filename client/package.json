{
  "name": "bingebox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:detailpage": "tailwindcss -c tailwind.config.js -i src/routes/DetailPage/detailpage.css -o src/routes/DetailPage/dist/detailpage.css --minify",
    "build:subscription": "tailwindcss -c tailwind.config.js -i src/routes/Subscription/subscription.css -o src/routes/Subscription/dist/subscription.css --minify",
    "build:pages": "npm run build:detailpage && npm run build:subscription",
    "watch:pages": "npm run build:detailpage -- --watch & npm run build:subscription -- --watch"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/cli": "^4.1.16",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "vite": "^7.1.7"
  }
}
